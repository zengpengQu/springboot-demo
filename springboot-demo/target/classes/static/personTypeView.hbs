
{{#each statisticData.data}}
    <div
        {{#compare type_code 'ZZ'}} class="infobox infobox-green"{{/compare}}
        {{#compare type_code 'LT'}} class="infobox infobox-blue" {{/compare}}

        {{#compare type_code 'DF'}}class="infobox infobox-red"{{/compare}}
        {{#compare type_code 'LW'}}class="infobox infobox-pink"{{/compare}}
        {{#compare type_code 'BW'}}class="infobox infobox-blue"{{/compare}}

    >
        <div class="infobox-icon">
            <i
                  {{#compare type_code 'ZZ'}} class="ace-icon fa fa-user"{{/compare}}
                  {{#compare type_code 'LT'}} class="ace-icon fa fa-coffee"{{/compare}}

                  {{#compare type_code 'DF'}} class="ace-icon fa fa-home"{{/compare}}
                  {{#compare type_code 'LW'}} class="ace-icon fa fa-wrench"{{/compare}}
                  {{#compare type_code 'BW'}} class="ace-icon fa fa-bookmark"{{/compare}}
           ></i>
        </div>

        <div class="infobox-data">
            <span class="infobox-data-number">{{#if count}}{{count}}{{else}}0{{/if}} äºº</span>
            <div class="infobox-content" style="font-weight: bold">{{type_name}}</div>
        </div>

    </div>

{{/each}}
